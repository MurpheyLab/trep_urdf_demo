            <mass value="0.1585"/>
            <inertia ixx="0"  ixy="0"  ixz="0" iyy="0" iyz="0" izz="0" />
        </inertial>
    </link>  

    <!-- right hip -->
<!--    <joint name="right_hip_psi" type="continuous" >-->
<!--        <origin xyz="0.065 0 -0.32647" />-->
