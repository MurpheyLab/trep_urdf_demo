    <joint name="right_hip_theta" type="continuous" >
        <origin xyz="-0.065 0 -0.32647" />
        <parent link="torso"/>
        <child link="right_hip_theta_link"/>
        <axis xyz="0 1 0" />
        <dynamics damping="0.005" />
    </joint>
    <link name="right_hip_theta_link" />
    <joint name="right_hip_phi" type="continuous" >
