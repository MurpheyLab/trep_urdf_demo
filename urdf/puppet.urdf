            <inertia ixx="0"  ixy="0"  ixz="0" iyy="0" iyz="0" izz="0" />
        </inertial>
    </link>

    <!-- left knee -->
    <joint name="left_knee_phi" type="continuous" >
        <origin xyz="0.0 0.0 -0.35" />
        <parent link="left_femur" />
        <child link="left_tibia" />
